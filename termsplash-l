# Create a [1280x1024] screen full of terminal panes

# Left pane
/usr/bin/gnome-terminal --geometry=84x18+3+3 &
/usr/bin/gnome-terminal --geometry=84x28+3+304 &
/usr/bin/gnome-terminal --geometry=84x14+3+756 &

# Right pane
/usr/bin/gnome-terminal --geometry=89x10+624+3 &
/usr/bin/gnome-terminal --geometry=89x52+624+186 &
